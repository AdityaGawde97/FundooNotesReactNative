{
  "name": "fundoonotes",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "android": "react-native run-android",
    "ios": "react-native run-ios",
    "start": "react-native start",
    "test": "jest --verbose --coverage",
    "test:update": "jest --verbose --coverage --updateSnapshot",
    "test:watch": "jest --verbose --watch",
    "coverage": "jest --verbose --coverage && open ./coverage/lcov-report/index.html",
    "lint": "eslint ."
  },
  "dependencies": {
    "@react-native-community/async-storage": "^1.7.1",
    "@react-native-community/datetimepicker": "^2.1.0",
    "@react-native-community/masked-view": "^0.1.6",
    "firebase": "^7.6.2",
    "lottie-react-native": "^3.3.2",
    "material-bread": "^0.2.5",
    "moment": "^2.24.0",
    "native-base": "^2.13.8",
    "react": "16.9.0",
    "react-native": "0.61.5",
    "react-native-animatable": "^1.3.3",
    "react-native-animated-loader": "0.0.8",
    "react-native-chart-kit": "^4.5.0",
    "react-native-color-palette": "^2.2.0",
    "react-native-dialog": "^5.6.0",
    "react-native-draggable-flatlist": "^2.2.0",
    "react-native-elements": "^1.2.7",
    "react-native-enhanced-popup-menu": "^0.6.1",
    "react-native-facebook-login-page": "^1.0.5",
    "react-native-gesture-handler": "^1.5.3",
    "react-native-hooks": "^0.9.0",
    "react-native-image-picker": "^2.2.1",
    "react-native-mail": "^4.1.0",
    "react-native-material-textfield": "^0.16.1",
    "react-native-modal": "^11.5.3",
    "react-native-modal-datetime-picker": "^8.1.1",
    "react-native-paper": "^3.4.0",
    "react-native-popup-dialog": "^0.18.3",
    "react-native-push-notification": "^3.1.9",
    "react-native-raw-bottom-sheet": "^2.0.6",
    "react-native-reanimated": "^1.7.0",
    "react-native-safe-area-context": "^0.6.2",
    "react-native-screens": "^2.0.0-beta.1",
    "react-native-svg": "^10.1.0",
    "react-native-vector-icons": "^6.6.0",
    "react-navigation": "^4.0.10",
    "react-navigation-animated-switch": "^0.4.1",
    "react-navigation-drawer": "^2.3.3",
    "react-navigation-stack": "^2.0.15",
    "react-redux": "^7.1.3",
    "redux": "^4.0.5"
  },
  "devDependencies": {
    "@babel/core": "^7.8.3",
    "@babel/runtime": "^7.8.3",
    "@react-native-community/eslint-config": "^0.0.5",
    "babel-jest": "^24.9.0",
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.15.2",
    "eslint": "^6.5.1",
    "jest": "^24.9.0",
    "jest-environment-enzyme": "^7.1.2",
    "jest-enzyme": "^7.1.2",
    "metro-react-native-babel-preset": "^0.56.0",
    "react-dom": "^16.12.0",
    "react-test-renderer": "16.9.0"
  },
  "jest": {
    "preset": "react-native",
    "verbose": true,
    "setupFilesAfterEnv": [
      "jest-enzyme"
    ],
    "testEnvironment": "enzyme",
    "testEnvironmentOptions": {
      "enzymeAdapter": "react16"
    },
    "cacheDirectory": "./cache",
    "coveragePathIgnorePatterns": [
      "./app/utils/vendor"
    ],
    "coverageThreshold": {
      "global": {
        "statements": 50
      }
    },
    "transformIgnorePatterns": [
      "node_modules/(?!(jest-)?react-native|react-clone-referenced-element|@react-native-community|expo(nent)?|@expo(nent)?/.*|react-navigation|@react-navigation/.*|@unimodules/.*|unimodules|sentry-expo|native-base|@react-native-firebase/.*|react-native-firebase)"
    ],
    "setupFiles": [
      "./jestSetUpFiles.js"
    ]
  }
}
